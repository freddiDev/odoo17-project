<odoo>
    <data noupdate="1">
        <record id="ir_cron_expire_loyalty_points" model="ir.cron">
            <field name="name">Check Expired Loyalty Points</field>
            <field name="model_id" ref="model_pos_loyalty_point"/>
            <field name="state">code</field>
            <field name="code">model.cron_check_expired_points()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="active" eval="True"/>
        </record>
    </data>
</odoo>
