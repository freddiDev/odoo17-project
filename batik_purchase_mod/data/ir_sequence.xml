<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="seq_rfq_number" model="ir.sequence">
        <field name="name">RFQ Number</field>
        <field name="code">purchase.order.rfq</field>
        <field name="padding">3</field>
        <field name="number_increment">1</field>
        <field name="company_id" eval="False"/>
    </record>
    
    <record id="seq_po_number" model="ir.sequence">
        <field name="name">Vendor Logistic Number</field>
        <field name="code">vendor.logistic.code</field>
        <field name="padding">3</field>
        <field name="number_increment">1</field>
        <field name="company_id" eval="False"/>
    </record>

    <record id="seq_purchase_summary" model="ir.sequence">
        <field name="name">Purchase Summary</field>
        <field name="code">purchase.summary</field>
        <field name="padding">3</field>
        <field name="number_increment">1</field>
        <field name="company_id" eval="False"/>
    </record>  
    <record id="seq_purchase_batik_order" model="ir.sequence">
        <field name="name">Purchase Order Batik Seq</field>
        <field name="code">purchase.order.batik.seq</field>
        <field name="padding">3</field>
        <field name="number_increment">1</field>
        <field name="company_id" eval="False"/>
    </record>
      
</odoo>
