<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="view_res_company_form_inherit_qc" model="ir.ui.view">
        <field name="name">res.company.form.inherit.expedition</field>
        <field name="model">res.company</field>
        <field name="inherit_id" ref="base.view_company_form"/>
        <field name="arch" type="xml">
            <xpath expr="//group[@name='social_media']" position="after">
                <group string="QC Percentage" name="qc_percentage" groups="base.group_no_one">
                    <field name="qc_percentage" class="o_light_label"/>
                </group>
            </xpath>
        </field>
    </record>
</odoo>