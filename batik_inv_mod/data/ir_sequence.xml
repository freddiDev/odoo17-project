<?xml version="1.0" encoding="utf-8"?>
<odoo>

     <data noupdate="1">
        <record id="seq_code_motif_number" model="ir.sequence">
            <field name="name">Code Motif</field>
            <field name="code">batik.code.seq.motif</field>
            <field name="padding">3</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>

    <data noupdate="0">
        <record id="seq_product_multiple_code_number" model="ir.sequence">
            <field name="name">Seq Product Code Wiz</field>
            <field name="code">batik.wiz.product.code.seq.model</field>
            <field name="padding">6</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_interwarehouse_transfer" model="ir.sequence">
            <field name="name">Interwarehouse Transfer</field>
            <field name="code">interwarehouse.transfer.seq.model</field>
            <field name="prefix">ITR/%(y)s%(month)s/</field>
            <field name="padding">6</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>

    <data noupdate="1">
        <record id="seq_vendor_return" model="ir.sequence">
            <field name="name">Seq Vendor Return</field>
            <field name="code">batik.code.seq.vendor.return</field>
            <field name="padding">3</field>
        </record>
    </data>
    
    <data noupdate="0">
        <record id="seq_interwarehouse_transfer_auditor" model="ir.sequence">
            <field name="name">Interwarehouse Transfer Auditor</field>
            <field name="code">interwarehouse.transfer.auditor.seq.model</field>
            <field name="padding">6</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>

         <record id="seq_interlocation_transfer" model="ir.sequence">
            <field name="name">Inter Location Transfer</field>
            <field name="code">interlocal.transfer.seq.model</field>
            <field name="padding">6</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
    <record id="seq_logistics_header" model="ir.sequence">
        <field name="name">Logistics Header</field>
        <field name="code">logistics.header</field>
        <field name="prefix">LG/%(y)s/</field>
        <field name="padding">4</field>
        <field name="company_id" eval="False"/>
    </record>

</odoo>
