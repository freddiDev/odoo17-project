<odoo>
  <data noupdate="1">

    <record id="cron_update_leadtime_actual" model="ir.cron">
      <field name="name">Update Leadtime Actual (Partner)</field>
      <field name="model_id" ref="base.model_res_partner"/>
      <field name="state">code</field>
      <field name="code">model.cron_update_leadtime_actual()</field>
      <field name="interval_number">1</field>
      <field name="interval_type">days</field>
      <field name="numbercall">-1</field>
      <field name="active">True</field>
      <field name="nextcall" eval="(datetime.now() + relativedelta(days=1)).replace(hour=0, minute=0, second=0).strftime('%Y-%m-%d %H:%M:%S')"/>
    </record>

  </data>
</odoo>
